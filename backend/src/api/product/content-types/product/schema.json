      "required": true
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "featured": {
      "type": "boolean",
      "default": false
    },
    "availablestock": {
      "type": "integer",
      "required": true
    }
  }
}
